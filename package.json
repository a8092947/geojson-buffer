{
  "name": "geojson-buffer",
  "version": "0.0.1",
  "description": "A js library for buffering geojson geometries",
  "main": "dist/geojson-buffer.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "bench": "npm run build && node test/benchmark.js",
    "build:docs": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "rollup -c",
    "debug": "cross-env webpack-dev-server --config debug/webpack.config.js --mode development --open --hot",
    "test": "ava --verbose",
    "test:regen": "cross-env REGEN=true ava"
  },
  "author": "Rowan Winsemius",
  "license": "MIT",
  "ava": {
    "cache": false,
    "require": [
      "@babel/register"
    ],
    "files": [
      "./test/**/*.spec.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@turf/helpers": "^6.1.4",
    "ava": "^1.2.1",
    "babel-loader": "^8.0.5",
    "benchmark": "^2.1.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.11",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.0.84",
    "file-loader": "^1.1.11",
    "iview": "^3.2.1",
    "js-angusj-clipper": "^1.0.1",
    "json-loader": "^0.5.7",
    "jsts": "^2.0.3",
    "load-json-file": "^5.1.0",
    "polygon-offset": "^0.3.1",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-butternut": "^0.1.0",
    "rollup-plugin-commonjs": "8.4.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "style-loader": "^0.23.1",
    "ttf-loader": "^1.0.2",
    "vue": "^2.6.6",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "write-json-file": "^3.0.2"
  },
  "dependencies": {
    "delaunator": "^3.0.2",
    "earcut": "^2.1.5",
    "leaflet": "^1.4.0",
    "libtess": "^1.2.2",
    "martinez-polygon-clipping": "^0.5.0",
    "polybooljs": "^1.2.0",
    "polygon-clipping": "^0.14.0",
    "shamos-hoey": "0.0.9"
  }
}
